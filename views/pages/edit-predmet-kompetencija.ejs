<%- include ('../partials/header'); -%>
<br>
<h4>IZMENI PREDMETNE KOMPETENCIJE:</h4>
<form method="post">
        <div class="form-group row">
            <label for="KO_NAZIV" class="col-2 col-form-label">Naziv</label>
            <div class="col-10">
                <input type="text" name="KO_NAZIV" value="<%= item.KO_NAZIV %>" class="form-control">
            </div>
        </div>
        <div class="form-group row">
            <label for="KO_OPIS" class="col-2 col-form-label">Opis</label>
            <div class="col-10">
                <input type="text" name="KO_OPIS" value="<%= item.KO_OPIS %>" class="form-control">
            </div>
        </div>
        <div class="form-group row">
            <label class="col-2 col-form-label">&nbsp;</label>
            <div class="col-10">
                <input type="submit" name="submit" value="Sačuvaj" class="btn btn-primary btn-lg">
            </div>
        </form>
</div>
<%- include ('../partials/footer'); -%>