<%- include ('../partials/header'); -%>
<br>
<div class="row" style="padding: 20px;">
    <form method="post" class="col s12">
        <h3><%= siteTitle %></h3>
        <br>
        
        <div class="form-group row">
            <label for="pgo" class="col-2 col-form-label">Kompetencija</label>
            <div class="col-10">
                <select class="form-control" name="kompetencija">
                    <% kompetencije.forEach(function(k) { %>
                    <option value="<%= k.PO_POLJE + '/' + k.GRU_GRUPA + '/' + k.OBL_OBLAST + '/' + k.KOMP_KATEGORIJA + '/' + k.KO_KOMPETENCIJA %>">
                        <%= k.KO_NAZIV %>
                    </option>
                    <% }); %>
                </select>
            </div>
        </div>

        <div class="form-group row">
            <label class="col-2 col-form-label">&nbsp;</label>
            <div class="col-10">
                <input type="submit" name="submit" value="Dodaj" class="btn btn-primary btn-lg">
            </div>
        </div>
    </form>
</div>
<%- include ('../partials/footer'); -%>
